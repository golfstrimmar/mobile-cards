section#search.search.home-section
    .container
        .title-1 
          h2  Поздравления по разделам
        ul.search__cards.search__cards--search.search-cards
          - 
            items = [
                { title: "С днём рождения ", url: "assets/img/home/img-5.svg" },
                { title: "С праздниками ", url: "assets/img/home/img-4.svg" },
                { title: "Со свадьбой ", url: "assets/img/home/img-8.svg" },
                { title: "Тосты ", url: "assets/img/home/img-9.svg" },
                { title: "Музыкальные и голосовые ", url: "assets/img/home/img-7.svg" },
                { title: "Картинки и открытки ", url: "assets/img/home/img-6.svg" },
              ]
          
          each item in items
              li.search-cards__card.search-cards__card--search.search-cards-card.random-color
                a(href="#!").search-cards-card__link.search-cards-card__link--search 
                  img(src=item.url,alt="img").search-cards-card__img.search-cards-card__img--search
                  .search-cards-card__title.search-cards-card__title--search=item.title 



